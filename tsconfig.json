{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@/src" : ["src/*"]
    },
    "compilerOptions": {
      "resolveJsonModule": true,
      "esModuleInterop": true,
      // other options
    }
  },
  "extends": "expo/tsconfig.base"
}
